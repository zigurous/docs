{
  "metadata": [],
  "build": {
    "content": [
      {
        "files": [
          "documentation/**.md",
          "documentation/**.yml",
          "toc.yml",
          "*.md"
        ]
      }
    ],
    "resource": [
      {
        "files": [
          "images/**",
          "favicon.ico",
          "logo.svg",
          "index.html"
        ]
      }
    ],
    "dest": "dist",
    "globalMetadata": {
      "_appTitle": "Documentation",
      "_appFooter": "Copyright Â© 2021 Zigurous",
      "_appLogoPath": "logo.svg",
      "_appFaviconPath": "favicon.ico",
      "_enableSearch": true,
      "_enableNewTab": true,
      "_disableContribution": true,
      "_gitContribute": {
        "repo": "https://github.com/zigurous/docs",
        "branch": "main"
      }
    },
    "template": [
      "default",
      "templates/zigurous"
    ],
    "postProcessors": [],
    "markdownEngineName": "markdig",
    "noLangKeyword": false,
    "keepFileLink": false,
    "cleanupCacheHistory": true,
    "disableGitFeatures": false
  }
}
